<?xml version="1.0" encoding="UTF-8"?>

<web-app>
  <servlet>
    <servlet-name>CsvUploadServlet</servlet-name>
    <servlet-class>com.example.CsvUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CsvUploadServlet</servlet-name>
    <url-pattern>/uploadCsv</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DisplayCsvServlet</servlet-name>
    <servlet-class>com.example.DisplayCsvServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayCsvServlet</servlet-name>
    <url-pattern>/displayCsv</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
