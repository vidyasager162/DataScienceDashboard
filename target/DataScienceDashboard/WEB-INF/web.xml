<?xml version="1.0" encoding="UTF-8"?>

<web-app>

  <jsp-config>
    <taglib>
      <taglib-uri>http://jakarta.apache.org/jsp/jstl/core</taglib-uri>
      <taglib-location>/WEB-INF/tld/c JSTL-core.tld</taglib-location>
    </taglib>
    <taglib>
      <taglib-uri>http://jakarta.apache.org/jsp/jstl/fmt</taglib-uri>
      <taglib-location>/WEB-INF/tld/c JSTL-fmt.tld</taglib-location>
    </taglib>
  </jsp-config>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
